{
  "name": "express-typescript-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build_project": "babel ./src --out-dir build --extensions '.ts,.tsx'",
    "build": "NODE_ENV=prod npm run build_project",
    "build_pre": "NODE_ENV=pre npm run build_project",
    "build_dev": "NODE_ENV=dev npm run build_project",
    "start": "NODE_ENV=local nodemon --watch 'src/**/*' --exec babel-node --extensions '.ts' ./src/index.ts"
  },
  "author": "Eduard Ivanov <smithjke@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "express": "4.17.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/body-parser": "1.17.0",
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.17.0",
    "@types/uuid": "3.4.4",
    "babel-plugin-module-resolver": "3.2.0",
    "nodemon": "1.19.1"
  }
}
